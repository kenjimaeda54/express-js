<% if(erros.length > 0){ %>
<div class="row">
  <div class="col">
    <div class="alert alert-danger">
     <%  erros.forEach(it => { %>
        <small>   <%= it %>   </small>  </br>       
    <% })  %>
    </div>
  </div>
</div>
<%  } %>

<% if(success.length > 0 ){ %>
<div class="row">
  <div class="col">
    <div class="alert alert-success">
        <small>   <%= success %>   </small>       
    </div>
  </div>
</div>
<% } %>
